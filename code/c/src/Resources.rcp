FORM ID GPSForm AT (0 0 160 160)
NOFRAME
MENUID GPSMenu
BEGIN
	TITLE "GPS"

	FIELD ID GPSStatusField AT (55 1 25 AUTO) NONEDITABLE FONT 0

	PUSHBUTTON "_ ON " ID StartPushButton AT (RIGHT@159 1 AUTO 12) GROUP GPSPushButtonGroup
	PUSHBUTTON "_OFF" ID StopPushButton AT (RIGHT@PREVLEFT-1 PREVTOP PREVWIDTH PREVHEIGHT) GROUP GPSPushButtonGroup
	PUSHBUTTON "_SIM" ID SimPushButton AT (RIGHT@PREVLEFT-1 PREVTOP PREVWIDTH PREVHEIGHT) GROUP GPSPushButtonGroup

	LABEL "HDOP:" AUTOID AT (2 16) FONT 0
	FIELD ID GPSHDOPField AT (PREVRIGHT+2 PREVTOP 20 AUTO) NONEDITABLE FONT 1 MAXCHARS 4

	LABEL "VDOP:" AUTOID AT (PREVRIGHT+4 PREVTOP) FONT 0
	FIELD ID GPSVDOPField AT (PREVRIGHT+2 PREVTOP PREVWIDTH AUTO) NONEDITABLE FONT 1 MAXCHARS 4

	LABEL "PDOP:" AUTOID AT (PREVRIGHT+4 PREVTOP) FONT 0
	FIELD ID GPSPDOPField AT (PREVRIGHT+2 PREVTOP PREVWIDTH AUTO) NONEDITABLE FONT 1 MAXCHARS 4

	FIELD ID LatField AT (1 110 79 AUTO) NONEDITABLE MAXCHARS 15 FONT 1
	FIELD ID LonField AT (81 PREVTOP 79 AUTO) NONEDITABLE MAXCHARS 15 FONT 1
	FIELD ID AltField AT (20 PREVBOTTOM 60 AUTO) NONEDITABLE MAXCHARS 10 FONT 1
	FIELD ID MagField AT (CENTER@120 PREVTOP 45 AUTO) NONEDITABLE MAXCHARS 5 FONT 1

	LABEL "_Time Zone:" AUTOID AT (RIGHT@60 PREVBOTTOM+1) FONT 0
	FIELD ID GPSTimeZoneField AT (PREVRIGHT+2 PREVTOP 28 AUTO) NONEDITABLE
	BUTTON "-" ID GPSTimeZoneMinusButton AT (PREVRIGHT+4 PREVTOP-1 AUTO AUTO)
	BUTTON "+" ID GPSTimeZonePlusButton AT (PREVRIGHT+4 PREVTOP AUTO AUTO)
	
	LABEL "_UTC:" AUTOID AT (25 PREVBOTTOM+1) FONT 0
	FIELD ID GPSUTCField AT (PREVRIGHT+2 PREVTOP 28 AUTO) FONT 1 NONEDITABLE MAXCHARS 4
	LABEL "_Local:" AUTOID AT (PREVRIGHT+7 PREVTOP) FONT 0
	FIELD ID GPSLocalField AT (PREVRIGHT+2 PREVTOP 30 AUTO) FONT 1 NONEDITABLE MAXCHARS 4

END

FORM ID FlightPlanForm AT (0 0 160 160)
NOFRAME
MENUID FlightPlanMenu
BEGIN
	TITLE "_Flight Plan"

	PUSHBUTTON "_Leg" ID FPLegPushButton AT (75 1 AUTO AUTO) GROUP FPLegPushButtonGroup
	PUSHBUTTON "_Tot" ID FPCumulativePushButton AT (PREVRIGHT PREVTOP AUTO AUTO) GROUP FPLegPushButtonGroup

	PUSHBUTTON "_ETE" ID FPETEPushButton AT (PREVRIGHT+4 PREVTOP AUTO AUTO) GROUP FPTimePushButtonGroup
	PUSHBUTTON "_ETA" ID FPETAPushButton AT (PREVRIGHT PREVTOP AUTO AUTO) GROUP FPTimePushButtonGroup


	LABEL "_Leg" AUTOID AT (2 19) FONT 1
	LABEL "_Alt" AUTOID AT (PREVLEFT+FPLegWidth PREVTOP) FONT 1
	LABEL "_Trk" AUTOID AT (PREVLEFT+FPAltWidth PREVTOP) FONT 1
	LABEL "_Dist" AUTOID AT (PREVLEFT+FPTrkWidth PREVTOP) FONT 1
	LABEL "_Time" AUTOID AT (PREVLEFT+FPDistWidth PREVTOP) FONT 1

	LIST "" ID FPLegList AT (1 PREVBOTTOM+1 158 95) FONT 6

	FIELD ID FPStatusField AT (0 PREVBOTTOM+1 FPLegWidth+FPAltWidth+FPTrkWidth AUTO) FONT 1 NONEDITABLE

	FIELD ID FPTotalDistance AT (FPLegWidth+FPAltWidth+FPTrkWidth+3 PREVTOP FPDistWidth AUTO) FONT 7 NONEDITABLE
	FIELD ID FPTotalTime AT (PREVLEFT+FPDistWidth PREVTOP 35 AUTO) FONT 7 NONEDITABLE

	BUTTON "A<->B" ID FPAlternateButton AT (1 BOTTOM@158 AUTO AUTO) FONT 0
#ifdef AEROPALM
	BUTTON "_Plan..." ID FPCoPilotButton AT (RIGHT@158 PREVTOP AUTO AUTO) FONT 0 
#else
	BUTTON "_CoPilot..." ID FPCoPilotButton AT (RIGHT@158 PREVTOP AUTO AUTO) FONT 0 
#endif

END

FORM ID EditWaypointForm AT (2 2 156 156)
MODAL
HELPID EditWaypointFormHelpString
BEGIN
	TITLE "_Edit Waypoint"
	LABEL "_Identifier:" AUTOID AT (RIGHT@58 15) FONT 1
	FIELD ID IdField AT (PREVRIGHT+2 PREVTOP 54 AUTO) MAXCHARS 10 UNDERLINED

	LABEL "_Latitude:" AUTOID AT (RIGHT@58 PREVBOTTOM+3) FONT 1
	PUSHBUTTON "N" ID NorthPushButton AT (PREVRIGHT+1 PREVTOP 12 AUTO) GROUP LatPushButtonGroup
	PUSHBUTTON "S" ID SouthPushButton AT (PREVRIGHT PREVTOP 12 AUTO) GROUP LatPushButtonGroup
	FIELD ID LatDegreesField AT (PREVRIGHT+2 PREVTOP 16 AUTO) MAXCHARS 3 UNDERLINED NUMERIC RIGHTALIGN
	LABEL "\260" AUTOID AT (PREVRIGHT+2 PREVTOP)
	FIELD ID LatMinutesField AT (PREVRIGHT+2 PREVTOP 24 AUTO) MAXCHARS 5 UNDERLINED NUMERIC RIGHTALIGN
	LABEL "'" AUTOID AT (PREVRIGHT+2 PREVTOP)

	LABEL "_Longitude:" AUTOID AT (RIGHT@58 PREVBOTTOM+3) FONT 1
	PUSHBUTTON "W" ID WestPushButton AT (PREVRIGHT+1 PREVTOP 12 AUTO) GROUP LonPushButtonGroup
	PUSHBUTTON "E" ID EastPushButton AT (PREVRIGHT PREVTOP 12 AUTO) GROUP LonPushButtonGroup
	FIELD ID LonDegreesField AT (PREVRIGHT+2 PREVTOP 16 AUTO) MAXCHARS 3 UNDERLINED NUMERIC RIGHTALIGN
	LABEL "\260" AUTOID AT (PREVRIGHT+2 PREVTOP)
	FIELD ID LonMinutesField AT (PREVRIGHT+2 PREVTOP 24 AUTO) MAXCHARS 5 UNDERLINED NUMERIC RIGHTALIGN
	LABEL "'" AUTOID AT (PREVRIGHT+2 PREVTOP)

	LABEL "_Mag.Var'n:" AUTOID AT (RIGHT@58 PREVBOTTOM+3) FONT 1
	FIELD ID MagVarnField AT (PREVRIGHT+1 PREVTOP 35 AUTO) MAXCHARS 6 UNDERLINED
	LABEL "_Notes:" AUTOID AT (6 PREVBOTTOM+5) FONT 1
	FIELD ID DescriptionField AT (6 PREVBOTTOM 142 48) MAXCHARS 1000 MULTIPLELINES UNDERLINED

	BUTTON "_OK" ID OKButton AT (5 BOTTOM@151 AUTO AUTO)
	BUTTON "_Exit" ID CancelButton AT (PREVRIGHT+6 PREVTOP AUTO AUTO)
	BUTTON "_Delete..." ID DeleteButton AT (PREVRIGHT+6 PREVTOP AUTO AUTO)
	GRAFFITISTATEINDICATOR AT (144 140)
END

