ERROR
BEGIN
	TITLE "_Input Error"
	MESSAGE "_^1 must be 0 to ^2 ^3"
	BUTTONS "_OK"
END

ALERT CoPilotWaypointsAlert
ERROR
BEGIN
	TITLE "_Too Many Waypoints"
	MESSAGE "_The number of waypoints in a database"\
		"_ is limited to 32767 (this is a PalmOS limitation)."
	BUTTONS "_Quit"
END

ALERT AsImportFailedAlert
ERROR
BEGIN
	TITLE "_Failed to Import Database"
	MESSAGE "^1"
	BUTTONS "_OK"
END

ALERT ConfirmImportAlert
INFORMATION
BEGIN
	TITLE "_Confirm Import"
	MESSAGE "_Are you sure you want to import"\
		"_ the ^1 database?"

	BUTTONS "_Yes" "_No"

END

ALERT DemoLeftAlert
INFORMATION
BEGIN
	TITLE "_Demo Version"
	MESSAGE "_Valid until: ^1"
	BUTTONS "_OK"
END

ALERT DemoExpiredAlert
WARNING
BEGIN
	TITLE "_Demo Expired"
	MESSAGE "_This demo version has expired."
	BUTTONS "_Quit"
END

ALERT LoadAlternateAlert
INFORMATION
BEGIN
	TITLE "_Load Alternate"
	MESSAGE "_Load alternative flight plan into standby plan (B)?"
	BUTTONS "_Yes" "_No"
END

ALERT FlightExistsAlert
WARNING
BEGIN
	TITLE "_Flight Exists"
	MESSAGE "_A flight with that name already exists, do you want "\
			"_to overwrite it?"
	BUTTONS "_Yes" "_No"
END

ALERT FlightDeleteAlert
WARNING
BEGIN
	TITLE "_Delete Flight?"
	MESSAGE "_Do you really want to delete this flight?"
	BUTTONS "_Yes" "_No"
END

ALERT RegcodeTesterAlert
INFORMATION
BEGIN
	TITLE "_Test"
	MESSAGE "^1"
	BUTTONS "_OK"
END

